link: https://strudel.cc/#%3D

setCpm(160/4)

$drums: stack (
  s("bd bd bd bd*2").bank("RolandTR909").gain(1).duckorbit("1:3"),
  //s("bd bd*2 bd bd/2").gain(0.5),
  s("[hh*2 hh hh*2 hh*2]").bank("RolandTR808").gain(1),
  //s("cp").beat("16", 32).bank("RolandTR808")
).orbit(2)._scope().lpf(5000)//.bank("RolandTR808")

$bass: stack(
  s("supersaw!8").note("<c#3 g#2 c#3 [f2 d#2]>").lpf(300).orbit(7),
  s("supersaw!16").note("<c#2 g#1 c#2 [f1 d#1]>").lpf(300).gain(1),
)._pianoroll()
$mel: stack(
  //note("<[d#6 d#6] [g#5 g5] [d#6 d#6 d#6 a#5] [b#5 a#5] [a#5 a#5 a#5 c6] [g#5 g5] [d#6 g6] [g#5 g5] >").sound("sawtooth").lpf(350).orbit(7),
  note("<[[d#4 c4] [a#4 c4] [d#4 c4] [g4 g#4]]>").s("piano").lpf(250).gain(0.8)._scope(),
  //change g#4 to *2, f5 d#5
).room("<0 .2 .4 .6 .8 1>")._pianoroll()


